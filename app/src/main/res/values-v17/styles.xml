<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="editedModifiedText" parent="BaseEditedModifiedText">
        <item name="android:layout_alignParentEnd">true</item>
    </style>

    <style name="customCardView" parent="BaseCustomCardView">
        <item name="android:layout_marginStart">@dimen/cardViewMarginSides</item>
        <item name="android:layout_marginEnd">@dimen/cardViewMarginSides</item>
    </style>

    <style name="headerCardViewLinearLayout"  parent="BaseHeaderCardViewLinearLayout">
        <item name="android:layout_marginStart">30dp</item>
        <item name="android:layout_marginEnd">30dp</item>
    </style>

    <style name="rowCardViewRelativeLayout" parent="BaseRowCardViewRelativeLayout">
        <item name="android:layout_marginStart">24dp</item>
        <item name="android:layout_marginEnd">12dp</item>
    </style>

    <style name="rowCardViewTitle" parent="BaseRowCardViewTitle">
        <item name="android:layout_marginEnd">50dp</item>
    </style>

    <style name="rowCardViewSmallTitle" parent="BaseRowCardViewSmallTitle">
        <item name="android:layout_marginEnd">45dp</item>
    </style>

    <style name="priorityBookmark" parent="BasePriorityBookmark">
        <item name="android:layout_marginStart">2dp</item>
    </style>

    <style name="statusIcon" parent="BaseStatusIcon">
        <item name="android:layout_alignParentEnd">true</item>
    </style>

</resources>